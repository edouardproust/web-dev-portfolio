	5: {type: 'link', label: 'Become an author', url: path('author_register')},
} %}
    
<div class="menu header_main">

	<ul class="menu header_main">

		{% for item in menuItems %}
			<li class="item">
				{% include "_parts/menu/item/" ~ item.type ~ ".html.twig" %}
			</li>
		{% endfor %}

	</ul>

</div>

